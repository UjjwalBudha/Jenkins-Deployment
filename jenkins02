pipeline {
    agent any
    stages {
        stage("Build Nodejs Image") {
            steps {
                bat 'docker build -t nodejs .'
                bat 'docker ps'
            }
        }

        stage("Delivery of Image to Docker Hub") {
            steps {
                bat 'docker image tag nodejs ujjwalbudha000/nodejs'
                bat 'docker push ujjwalbudha000/nodejs:nodejs'
            }
        }

       
        }
    }
