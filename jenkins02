pipeline {
    agent any
    stages {
        stage("Build Nodejs Image") {
            steps {
                bat 'docker build -t nodejs:1.2 .'
                bat 'docker ps'
            }
        }

        stage("Delivery of Image to Docker Hub") {
            steps {
                bat 'docker tag nodejs:1.2 ujjwalbudha000/nodejs/nodejs:1.2'
                bat 'docker push ujjwalbudha000/nodejs/nodejs:1.2'
            }
        }

       
        }
    }
