pipeline {
    agent any
    stages {
        stage("Build Nodejs Image") {
            steps {
                bat 'docker build -t nodejs:1.2 .'
                bat 'docker images'
            }
        }

        stage("Delivery of Image to Docker Hub") {
            steps {
                bat 'docker tag nodejs:latest ujjwalbudha000/myrepo:v8'
                bat 'docker push ujjwalbudha000/myrepo:v8'
            }
        }

       
        }
    }
