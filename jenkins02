pipeline {
    agent any
    stages {
  
        stage("Build Nodejs Image") {
            steps {
                bat 'docker build -t nodejs .'
                bat 'docker images'
            }
        }
  
        stage("Delivery of Image to Docker Hub") {
            steps {
                bat 'docker tag nodejs:latest ujjwalbudha000/ujjwalbudha000:v8'
                bat 'docker push ujjwalbudha000/ujjwalbudha000:v8'
                bat 'docker login'
            }
        }
    }
}
